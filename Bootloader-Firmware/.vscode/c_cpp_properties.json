{
    "configurations": [
        {
            "name": "HW_Template",
            "includePath": [
                "${workspaceFolder}/**"
            ],
            "defines": [
				"STM32F103xB",
                "USE_HAL_DRIVER"
            ],
            "cStandard": "c17",
            "cppStandard": "c++17",
            "intelliSenseMode": "gcc-arm",
            "compilerPath": "${env:ARM-GCC}/arm-none-eabi-gcc.exe",
            "compilerArgs": [
                "-mcpu=cortex-m3",
                "-mthumb"
            ],
            "browse": {
                "limitSymbolsToIncludedHeaders": true
            }
        }
    ],
    "version": 4
}