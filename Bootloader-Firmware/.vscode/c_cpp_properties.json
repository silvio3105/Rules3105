{
    "configurations": [
        {
            "name": "HW_Template",
            "includePath": [
				"${workspaceFolder}/Hardware/HW_Template/**",
				"${workspaceFolder}/Hardware/HW_Template/Inc/**",
                "${workspaceFolder}/Application/Inc/**",
				"${workspaceFolder}/CMSIS/**",
				"${workspaceFolder}/Config/**",
				"${workspaceFolder}/Drivers/Inc/**",
				"${workspaceFolder}/Libraries/Inc/**",
				"${workspaceFolder}/Modules/Inc/**",
				"${workspaceFolder}/RTOS/RTX/Inc/**"
            ],
            "defines": [
				"HW_HW_Template",
				"HW_CONFIG=\"HW_Template.hpp\"",
				"STM32F103x8",
				"RTOS",
				"DEBUG",
				"DEBUG_VERBOSE",
				"DEBUG_INFO",
				"DEBUG_ERROR",
				"DEBUG_HANDLER_PRINT=Debug::log",
				"DEBUG_HANDLER_PRINTF=Debug::logf",
				"DEBUG_SRC=\"Debug.hpp\"",
				"DEBUG_PRINTF_STACK",
				"DEBUG_BUFFER_SIZE=128"
            ],
            "cStandard": "c17",
            "cppStandard": "c++17",
            "intelliSenseMode": "gcc-arm",
            "compilerPath": "${env:ARM-GCC}/arm-none-eabi-gcc.exe",
            "compilerArgs": [
                "-mcpu=cortex-m3",
                "-mthumb"
            ],
            "browse": {
                "limitSymbolsToIncludedHeaders": true
            }
        }
    ],
    "version": 4
}