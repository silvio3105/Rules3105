{
    "configurations": [
        {
            "name": "HW_Template",
            "includePath": [
                "${workspaceFolder}/**"
            ],
            "defines": [
				"BUILD_HW_Template",
				"STM32F103x8",
				"HW_CONFIG=\"HW_Template.hpp\"",
				"RTOS",
				"DEBUG",
				"DEBUG_SRC=\"Debug.hpp\"",
				"DEBUG_PRINT=Debug::log",
				"DEBUG_PRINTF=Debug::logf"
            ],
            "cStandard": "c17",
            "cppStandard": "c++17",
            "intelliSenseMode": "gcc-arm",
            "compilerPath": "${env:ARM-GCC}/arm-none-eabi-gcc.exe",
            "compilerArgs": [
                "-mcpu=cortex-m3",
                "-mthumb"
            ],
            "browse": {
                "limitSymbolsToIncludedHeaders": true
            }
        }
    ],
    "version": 4
}